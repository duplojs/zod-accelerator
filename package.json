{
  "name": "@duplojs/zod-accelerator",
  "version": "0.0.0",
  "author": "mathcovax",
  "description": "",
  "repository": "https://github.com/duplojs/duplojs-zod-accelerator",
  "license": "ISC",
  "keywords": [
    "typescript",
    "json-schema",
    "parser",
    "validate",
    "typecheck",
	"zod",
	"faster",
	"zod faster"
  ],
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
	  "types": "./types/index.d.ts"
    },
	"./plugin": {
	  "types": "./plugin.d.ts",
	  "import": "./dist/plugin.mjs",
	  "require": "./dist/plugin.cjs"
	}
  },
  "files": [
    "dist",
    "types",
	"plugin.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup --config && npx tsc -p tsconfig.build.json",
    "test:bench": "tsx benchmark/string.bench.ts && tsx benchmark/object.bench.ts && tsx benchmark/array.bench.ts && tsx benchmark/union.bench.ts",
    "test:tu:coverage": "npx vitest --coverage",
    "test:e2e": "node --require sucrase/register test/E2E",
    "test:types": "tsc"
  },
  "dependencies": {
    "@duplojs/duplojs": ">=1.8.20 <2.0.0",
    "@duplojs/editor-tools": "^1.1.3 <2.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@duplojs/worker-testing": "^0.3.6",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@sinclair/typebox": "^0.32.15",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vinejs/vine": "^1.8.0",
    "@vitest/coverage-istanbul": "^1.3.1",
    "autocannon": "^7.11.0",
    "eslint": "^8.45.0",
    "joi": "^17.12.2",
    "myzod": "^1.11.0",
    "rollup": "^3.26.2",
    "sucrase": "^3.34.0",
    "tinybench": "^2.6.0",
    "tslib": "^2.6.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "vitest": "^1.3.1"
  }
}
